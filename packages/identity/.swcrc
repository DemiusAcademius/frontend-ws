{
  "test": ".*.ts$",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "dynamicImport": true
      },
     "transform": {
       "legacyDecorator": true,
       "decoratorMetadata": true
     },
      "loose": true,
      "target": "es2016"
    }
  }